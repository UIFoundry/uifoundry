{
	"name": "uifoundry",
	"version": "0.1.1",
	"private": true,
	"license": "PolyForm-Perimeter-1.0.0",
	"type": "module",
	"scripts": {
		"build": "payload generate:types && payload generate:importmap && shadcn build && next build",
		"registry:build": "shadcn build",
		"check": "eslint && tsc --noEmit",
		"dev": "next dev --port=3005 --turbo",
		"dev:prod": "NODE_ENV=production NEXT_PUBLIC_BETTER_AUTH_URL=http://localhost:3005 next dev --port=3005 --turbo",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"vitest": "vitest --config ./vitest.config.ts",
		"test": "playwright test --config=playwright.config.ts",
		"test:prod:baseline": "k6 run src/test/k6/prod-baseline-test.js",
		"test:prod:homepage": "k6 run src/test/k6/prod-homepage-test.js",
		"test:prod:stress": "k6 run src/test/k6/prod-stress-test.js",
		"test:load": "k6 run src/test/k6/simple-load-test.js",
		"test:spike": "k6 run src/test/k6/simple-spike-test.js",
		"test:stress": "k6 run src/test/k6/simple-stress-test.js",
		"test:docs": "playwright test tests/docs-pages.spec.ts --config=playwright.config.ts",
		"test:docs:retry": "playwright test tests/docs-pages.spec.ts --config=playwright.config.ts --retries=3",
		"test:report": "playwright show-report",
		"payload:importmap": "payload generate:importmap",
		"payload:types": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
		"payload:update": "pnpm update 'payload' '@payloadcms/*'",
		"preview": "next build && next start",
		"sso": "aws sso login --sso-session=uifoundry",
		"refresh": "sst refresh --stage=production",
		"secret:set": "sst secret set",
		"secret:set:dev": "sst secret set --stage dev",
		"secret:set:prod": "sst secret set --stage production",
		"secret:remove:dev": "sst secret remove --stage dev",
		"secret:remove:prod": "sst secret remove --stage production",
		"secret:list:dev": "sst secret list --stage dev",
		"secret:list:prod": "sst secret list --stage production",
		"stripe:listen": "stripe listen --forward-to localhost:3005/api/auth/stripe/webhook",
		"deploy:dev": "pnpm vitest && sst deploy --stage dev",
		"deploy:prod": "NODE_ENV=production pnpm vitest && sst deploy --stage production",
		"start": "next start",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@better-auth/stripe": "^1.3.26",
		"@daveyplate/better-auth-ui": "^3.1.5",
		"@payloadcms/db-mongodb": "^3.59.1",
		"@payloadcms/graphql": "^3.59.1",
		"@payloadcms/live-preview-react": "^3.59.1",
		"@payloadcms/next": "^3.59.1",
		"@payloadcms/richtext-lexical": "^3.59.1",
		"@payloadcms/storage-s3": "^3.59.1",
		"@payloadcms/ui": "^3.59.1",
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.13",
		"@shadcn/ui": "^0.0.4",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-form": "^1.19.4",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"@types/mdx": "^2.0.13",
		"@uiw/react-color-sketch": "^2.7.3",
		"better-auth": "^1.3.6",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"culori": "^4.0.2",
		"fumadocs-core": "^15.7.10",
		"fumadocs-mdx": "^11.9.0",
		"fumadocs-typescript": "^4.0.8",
		"fumadocs-ui": "^15.7.10",
		"graphql": "^16.10.0",
		"lucide-react": "^0.525.0",
		"mongodb": "^6.18.0",
		"motion": "^12.23.12",
		"next": "16.0.0-canary.7",
		"next-themes": "^0.4.6",
		"payload": "^3.59.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-syntax-highlighter": "^15.6.6",
		"react-use-measure": "^2.1.7",
		"server-only": "^0.0.1",
		"sharp": "^0.34.1",
		"source-map-support": "^0.5.21",
		"sst": "3.17.14",
		"stripe": "^19.1.0",
		"superjson": "^2.2.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@next/eslint-plugin-next": "^15.4.3",
		"@playwright/test": "^1.55.0",
		"@tailwindcss/postcss": "^4.0.15",
		"@types/culori": "^4.0.1",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@types/react-syntax-highlighter": "^15.5.13",
		"babel-plugin-react-compiler": "19.1.0-rc.1",
		"bun": "^1.2.21",
		"dotenv-cli": "^10.0.0",
		"eslint": "^9.23.0",
		"eslint-config-next": "^15.2.3",
		"eslint-plugin-react-hooks": "^5.2.0",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"shadcn": "^3.2.1",
		"tailwindcss": "^4.0.15",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.27.0",
		"vite": "^7.1.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "pnpm@10.11.0",
	"pnpm": {
		"overrides": {}
	}
}
